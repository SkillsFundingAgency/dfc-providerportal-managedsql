{
    "name": "Execute SSIS transform",
    "properties":
    {
        "description": "This pipeline executes the tribal importer with the PP/PRD environment settings",
        "activities":
        [
            {
                "name": "Execute SSIS package on PP",
                "description": "Execute the tribal importer for the PP platform",
                "type": "ExecuteSSISPackage",
                "typeProperties":
                {
                    "packageLocation": {
                        "packagePath": "TribalImporter/TribalToNCS-Importer/TribalToNCSImporter.dtsx",
                        "type": "SSISDB"
                    },
                    "environmentPath": null,
                    "connectVia": {
                        "referenceName": "tribalSSIS",
                        "type": "IntegrationRuntimeReference"
                    },
                    "loggingLevel": "Basic",
                    "packageConnectionManagers": {
                        "SourceConnection": {
                            "ConnectionString": {
                                "value": "__SourceConnectionStringDevPP_"
                            }
                        },
                        "TargetConnection": {
                            "ConnectionString": {
                                "value": "__TargetConnectionStringPP__"
                            }
                        }
                    }
                },
                "policy":
                {
                    "timeout": "7.00:00:00",
                    "retry": 0,
                    "retryIntervalInSeconds": 30,
                    "secureOutput": false,
                    "secureInput": false
                }
            },
            {
                "name": "Execute SSIS package on PRD",
                "description": "Execute the tribal importer for the PRD platform",
                "type": "ExecuteSSISPackage",
                "typeProperties":
                {
                    "packageLocation": {
                        "packagePath": "TribalImporter/TribalToNCS-Importer/TribalToNCSImporter.dtsx",
                        "type": "SSISDB"
                    },
                    "environmentPath": null,
                    "connectVia": {
                        "referenceName": "tribalSSIS",
                        "type": "IntegrationRuntimeReference"
                    },
                    "loggingLevel": "Basic",
                    "packageConnectionManagers": {
                        "SourceConnection": {
                            "ConnectionString": {
                                "value": "__SourceConnectionStringPrd__"
                            }
                        },
                        "TargetConnection": {
                            "ConnectionString": {
                                "value": "__TargetConnectionStringPrd__"
                            }
                        }
                    }
                },
                "policy":
                {
                    "timeout": "7.00:00:00",
                    "retry": 0,
                    "retryIntervalInSeconds": 30,
                    "secureOutput": false,
                    "secureInput": false
                }
            }
        ]
    }
}